<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.report.Report" table="REPORT" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="name" type="string" column="NAME"/>
		<property name="category" type="string" column="CATEGORY"/>
		<property name="qrysql" type="string" column="QRYSQL"/>
		<property name="params" type="string" column="PARAMS"/>
		<property name="fields" type="string" column="FIELDS"/>
		<property name="displayname" type="string" column="DISPLAYNAME"/>
		<property name="maxnum" type="integer" column="MAXNUM"/>
		<property name="roles" type="string" column="ROLES"/>
		<property name="description" type="string" column="DESCRIPTION"/>
	</class>
</hibernate-mapping>
